<h1>My Event History</h1>
<tbody>
    <% @event_hists.each do |event_hist| %>
      <tr>
        <td><%= event_hist.event_id if event_hist.user_id == 1 %></td>
        <td><%= event_hist.user_id if event_hist.user_id == 1 %></td>
        <td><%= event_hist.sign_in if event_hist.user_id == 1 %></td>
        <td><%= event_hist.point_recv if event_hist.user_id == 1 %></td>
        <td><%= link_to 'Show', event_hist if event_hist.user_id == 1 %></td>
        <br>
      </tr>
    <% end %>
  </tbody>

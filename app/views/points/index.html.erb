<p id="notice"><%= notice %></p>

<h1>Points</h1>

<table>
  <thead>
    <tr>
      <th>Referrals</th>
      <th>Prof referrals</th>
      <th>Gen event</th>
      <th>Fund event</th>
      <th>Vol event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @points.each do |point| %>
      <tr>
        <td><%= point.referrals %></td>
        <td><%= point.prof_referrals %></td>
        <td><%= point.gen_event %></td>
        <td><%= point.fund_event %></td>
        <td><%= point.vol_event %></td>
        <td><%= link_to 'Show', point %></td>
        <td><%= link_to 'Edit', edit_point_path(point) %></td>
        <td><%= link_to 'Destroy', point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Point', new_point_path %>

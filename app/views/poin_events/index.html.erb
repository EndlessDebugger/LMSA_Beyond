<p id="notice"><%= notice %></p>

<h1>Poin Events</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Balance</th>
      <th>Date</th>
      <th>Description</th>
      <th>Admin award</th>
      <th>Hours attend</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poin_events.each do |poin_event| %>
      <tr>
        <td><%= poin_event.user_id %></td>
        <td><%= poin_event.balance %></td>
        <td><%= poin_event.date %></td>
        <td><%= poin_event.description %></td>
        <td><%= poin_event.admin_award_id %></td>
        <td><%= poin_event.hours_attend %></td>
        <td><%= link_to 'Show', poin_event %></td>
        <td><%= link_to 'Edit', edit_poin_event_path(poin_event) %></td>
        <td><%= link_to 'Destroy', poin_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poin Event', new_poin_event_path %>
